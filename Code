int temp1 = 0;

int temp2 = 0;

int temp3 = 0;

int i = 0;

int Iluminosidade = 0;

void setup()
{
  pinMode(A0, INPUT);
  Serial.begin(9600);
}

void loop()
{
  Iluminosidade = analogRead(A0);
  Serial.println(Iluminosidade);
  delay(10); // Delay a little bit to improve simulation performance
}
